               <xsd:documentation> Список ранее выданных карт </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Sign" maxOccurs="1" minOccurs="1" type="AttachmentsType">
            <xsd:annotation>
               <xsd:documentation> Изображение подписи </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>   
   </xsd:complexType>
   <xsd:complexType name="CompanyOrderType">
     <xsd:annotation>
        <xsd:documentation>Данные, если заявитель компания</xsd:documentation>
