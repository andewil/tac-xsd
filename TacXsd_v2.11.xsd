               <xsd:documentation> Почтовый адрес транспортного предприятия
                    </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Photo" maxOccurs="1" minOccurs="1" type="Attachments_v3Type">
            <xsd:annotation>
               <xsd:documentation>Фотография</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Sign" maxOccurs="1" minOccurs="1" type="Attachments_v3Type">
            <xsd:annotation>
