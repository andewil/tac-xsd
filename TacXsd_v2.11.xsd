        			</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrderDate" maxOccurs="1" minOccurs="1" type="xsd:date">
            <xsd:annotation>
               <xsd:documentation>
        				Дата подачи заявления
        			</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrderCardType" maxOccurs="1" minOccurs="1" type="OrderCardType">
            <xsd:annotation>
               <xsd:documentation>Тип карты</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CardNumber" maxOccurs="1" minOccurs="1" type="CardNumberType"/>
         <xsd:element name="OrderReason" maxOccurs="1" minOccurs="1">
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                 <xsd:enumeration value="create"/>
                 <xsd:enumeration value="change"/>
                 <xsd:enumeration value="refresh"/>
                 <xsd:enumeration value="newpd"/>
              </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:choice>
            <xsd:element name="RuPersonOrder" maxOccurs="1" minOccurs="0" type="RuPersonOrder_v3Type">
