   <xsd:complexType name="CardSimpleType">
     <xsd:annotation>
        <xsd:documentation>Информация о ранее выданной карте </xsd:documentation>
     </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="CardNumber" maxOccurs="1" minOccurs="1" type="CardNumberType">
            <xsd:annotation>
               <xsd:documentation>Номер карты</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CardExpiration" maxOccurs="1" minOccurs="0" type="xsd:date">
            <xsd:annotation>
               <xsd:documentation>Дата окончания действия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NameFirst" maxOccurs="1" minOccurs="0" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Имя</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NameMiddle" maxOccurs="1" minOccurs="0" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Отчество</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NameLast" maxOccurs="1" minOccurs="0" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Фамилия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>   
   </xsd:complexType>
   <xsd:simpleType name="CardNumberType">
     <xsd:restriction base="xsd:string">
        <xsd:length value="16"/>
        <xsd:pattern value="RU[DMPK][A-Z0-9]{4}\d{5}[A-Z0-9]{4}"/>
     </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="PersonSimpleType">
     <xsd:annotation>
        <xsd:documentation>Сотрудники </xsd:documentation>
     </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="NameFirst" maxOccurs="1" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Имя</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NameMiddle" minOccurs="1" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Отчество</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NameLast" minOccurs="1" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Фамилия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Position" maxOccurs="1" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Должность</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>   
   </xsd:complexType>
   <xsd:complexType name="PassportType">
     <xsd:annotation>
        <xsd:documentation>Данные паспорта</xsd:documentation>
     </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="PassportSeries" maxOccurs="1" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation> Серия паспорта </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                 <xsd:maxLength value="32"/>
              </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="PassportNumber" maxOccurs="1" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation> Номер паспорта </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                 <xsd:maxLength value="32"/>
              </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="PassportIssuedBy" maxOccurs="1" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation> Кем выдан паспорт </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                 <xsd:maxLength value="100"/>
              </xsd:restriction>
