               <xsd:documentation>Квартира/Офис</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                 <xsd:maxLength value="10"/>
              </xsd:restriction>
            </xsd:simpleType>
