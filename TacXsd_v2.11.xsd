                  <xsd:documentation>
        					Данные, если заявитель не компания
        				</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="RuCompanyOrder" maxOccurs="1" minOccurs="0" type="CompanyOrderType">
               <xsd:annotation>
                  <xsd:documentation>
        					Данные, если заявитель компания
        				</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="RuWorkshopOrder" maxOccurs="1" minOccurs="0" type="WorkshopOrderType">
               <xsd:annotation>
                  <xsd:documentation>
        					Данные, если заявитель мастерская
        				</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="RuSupervisorFsntOrder" maxOccurs="1" minOccurs="0" type="SupervisorOrderType">
               <xsd:annotation>
                  <xsd:documentation>
        					Данные, если заявитель котролирующая
        					организация
        				</xsd:documentation>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="RuSupervisorGibddOrder" type="SupervisorOrderType"/>
         </xsd:choice>
      </xsd:sequence>   
   </xsd:complexType>
   <xsd:complexType name="Attachments_v3Type">
