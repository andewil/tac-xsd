               <xsd:documentation> Почтовый адрес транспортного предприятия
					</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Photo" maxOccurs="1" minOccurs="1" type="AttachmentsType">
            <xsd:annotation>
               <xsd:documentation>Фотография</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Sign" maxOccurs="1" minOccurs="1" type="AttachmentsType">
            <xsd:annotation>
               <xsd:documentation> Изображение подписи </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>   
   </xsd:complexType>
   <xsd:complexType name="PersonOrder_v2Type">
     <xsd:annotation>
        <xsd:documentation>Данные, если заявитель частное лицо</xsd:documentation>
     </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="PersonInfo" maxOccurs="1" minOccurs="1" type="PersonInfo_v2Type">
