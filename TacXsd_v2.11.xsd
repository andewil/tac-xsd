            <xsd:annotation>
               <xsd:documentation>Улица</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                 <xsd:maxLength value="64"/>
              </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="House" maxOccurs="1" minOccurs="0">
