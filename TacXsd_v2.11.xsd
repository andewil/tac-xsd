               <xsd:documentation>Данные сертификата: Serial number</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CertIssuer" maxOccurs="1" minOccurs="1" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Данные сертификата: Issuer</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CertValidFrom" maxOccurs="1" minOccurs="1" type="xsd:dateTime">
            <xsd:annotation>
               <xsd:documentation>Данные сертификата: Valid From</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CertValidTo" maxOccurs="1" minOccurs="1" type="xsd:dateTime">
            <xsd:annotation>
               <xsd:documentation>Данные сертификата: Valid To</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CertSubject" maxOccurs="1" minOccurs="1" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Данные сертификата: Subject</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CertThumbprint" maxOccurs="1" minOccurs="0" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation>Данные сертификата: Thumbprint</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>   
   </xsd:complexType>
   <xsd:complexType name="CardInfoType">
     <xsd:annotation>
        <xsd:documentation>Информация о карте тахографа</xsd:documentation>
     </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="CardNumber" maxOccurs="1" minOccurs="1" type="CardNumberType">
            <xsd:annotation>
               <xsd:documentation>Номер карты</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IssueDate" maxOccurs="1" minOccurs="1" type="xsd:date">
            <xsd:annotation>
               <xsd:documentation>Дата начала действия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Expiration" maxOccurs="1" minOccurs="1" type="xsd:date">
            <xsd:annotation>
               <xsd:documentation>Дата окончания действия</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CertInfo" maxOccurs="1" minOccurs="0" type="CertInfoType">
            <xsd:annotation>
               <xsd:documentation>Информация о сертификате</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SKZI" maxOccurs="1" minOccurs="0">
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                 <xsd:pattern value="\d{3}-\d{7}"/>
              </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
      </xsd:sequence>   
   </xsd:complexType>
   <xsd:simpleType name="OrderNumberType">
     <xsd:annotation>
        <xsd:documentation>Номер заявления [A-Z]{5}-\d{8} ОВК-Порядковый номер формируется в ОВК
				при отправке изготовителю карт, затем участвует во всех стадиях информационного
				обмена </xsd:documentation>
     </xsd:annotation>
     <xsd:restriction base="xsd:string">
        <xsd:length value="14"/>
        <xsd:pattern value="[A-Z]{5}-\d{8}"/>
     </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="PersonOrderType">
     <xsd:annotation>
        <xsd:documentation>Данные, если заявитель не компания</xsd:documentation>
     </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="PersonInfo" maxOccurs="1" minOccurs="1" type="PersonInfoType">
            <xsd:annotation>
               <xsd:documentation>Персональные данные заявителя</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="PrevCardNumber" maxOccurs="1" minOccurs="0" type="CardNumberType">
            <xsd:annotation>
               <xsd:documentation>Номер ранее выданной карты</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ContactInfo" maxOccurs="1" minOccurs="0" type="ContactInfoType">
            <xsd:annotation>
               <xsd:documentation>Контактная информация</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DriverLicense" maxOccurs="1" minOccurs="1" type="DriverLicenseType">
            <xsd:annotation>
               <xsd:documentation>Данные водительского удостоверения</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrgName" maxOccurs="1" minOccurs="1" type="xsd:string">
            <xsd:annotation>
               <xsd:documentation> Наименование транспортного предприятия </xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OrgPostAddress" maxOccurs="1" minOccurs="1" type="OrgAddressType">
