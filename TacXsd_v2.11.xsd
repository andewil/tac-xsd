   <xsd:complexType name="OrgAddressType">
     <xsd:annotation>
        <xsd:documentation>Адрес организации</xsd:documentation>
     </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="FullAddress" maxOccurs="1" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation>
						Адрес в одну строку
					</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                 <xsd:maxLength value="128"/>
              </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
         <xsd:element name="PostCode" maxOccurs="1" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation>
						Почтовый индекс
					</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                 <xsd:pattern value="\d{6}"/>
                 <xsd:minLength value="6"/>
                 <xsd:maxLength value="6"/>
              </xsd:restriction>
            </xsd:simpleType>
         </xsd:element>
